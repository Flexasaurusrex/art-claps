# Copy to .env.local and fill in your values

# Database (Supabase)
DATABASE_URL="postgresql://postgres:[PASSWORD]@db.[REF].supabase.co:5432/postgres"

# NextAuth
NEXTAUTH_URL="http://localhost:3000"
NEXTAUTH_SECRET="your-super-secret-key-change-this-in-production"

# Farcaster OAuth (get from https://warpcast.com/~/developers)
FARCASTER_CLIENT_ID="your-client-id"
FARCASTER_CLIENT_SECRET="your-client-secret"

# Farcaster Hub (optional - for real-time tracking)
FARCASTER_HUB_URL="https://hub.farcaster.xyz"
FARCASTER_API_KEY="your-hub-api-key"

# Admin API (for background sync jobs)
ADMIN_API_KEY="your-secure-admin-key"
